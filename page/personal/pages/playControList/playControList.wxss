/* playControList.wxss */
.playControListWrap{
  font-family:Microsoft YaHei;
}
.playFilm{
  height: 100rpx;
  width: 94%;
  margin:0 auto;
  margin-top: 20rpx;
  background: rgb(27,199,97);
  line-height: 100rpx;
  color : #fff;
  font-weight: 600;
  font-size : 34rpx;
}
.play-btn{
  height: 50rpx;
  width: 50rpx;
  font-size: 0;
  display:block;
  float:right;
  margin-top:25rpx;
  margin-right:60rpx;
}
.room_name{
  float: left;
  width: 30%;
  overflow: hidden
}
.film_name{
  float: left;
  width: 30%;
  overflow: hidden
}
.playInfo{
  float: left;
  height: 100rpx;
  width: 80%;
  overflow: hidden;
  text-indent: 30rpx;
}
.playing-wrap{
  margin-top: 25rpx;
  border-top: 2rpx solid #ddd;
  border-bottom: 2rpx solid #ddd;
}











.play-film{
  width:100%;
  height:200rpx;
  overflow:hidden;
  background:#fff;
  padding:10rpx 0
}
.play-film-l{
  float:left;
  width:77%;
  overflow:hidden;
}
.top{
  margin-left:10%;
  height:98rpx;
  line-height:98rpx;
  font-size:0;
}
.room-name{
  display:inline;
  font-size:36rpx;
  font-weight:600;
  margin-left:10rpx;
  color:#2d2d2d;
  overflow: hidden;
}
.middle{
  margin-left:7%;
  height:2rpx;
  width:100%;
  background:#5fbe51;
}
.bottom{
  margin-left:10%;
  height:98rpx;
  line-height:98rpx;
  font-size:28rpx;
  color:#707070;
  overflow: hidden;
}
.play-film-r{
  float:right;
  margin-right:6%;
}
.room_icon{
  width:32rpx;
  height:32rpx;
}
.play_icon{
  width:80rpx;
  height:80rpx;
  margin-top:60rpx
}