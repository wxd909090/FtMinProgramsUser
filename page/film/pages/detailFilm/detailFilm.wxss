/* 热映 */
page{
    position: relative
}
.detail-banner-wrap{
    position: relative;
    overflow: hidden;
    background: black;
}
.detail-banner{
    width:94.66%;
    height:410rpx;
    padding:30rpx 2.67% 20rpx;
}
.detail-banner-bg{
    -webkit-filter: blur(13px);
    -moz-filter: blur(13px);
    -ms-filter: blur(13px);
    filter: blur(13px);
    position:relation;
}
.detail-banner-info{
    position:absolute;
    top:0;
    left:0;
}
.detail-banner .detail-banner-up{
