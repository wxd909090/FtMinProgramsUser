    white-space:nowrap;
    overflow:hidden;
    text-overflow:ellipsis;
}
.detail-banner .detail-banner-up .detail-banner-up-rig .detail-banner-up-rig-two-txt{
    color:#fffefe;
    height:20rpx;
    line-height:20rpx;
