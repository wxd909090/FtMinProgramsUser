  margin-top:24rpx;
}
.detail-banner .detail-banner-up .detail-banner-up-rig .detail-banner-up-rig-three-view image{
  height:28rpx;
  width:28rpx;
  margin-right:10rpx;
}
.detail-banner .detail-banner-up .detail-banner-up-rig .detail-banner-up-rig-three-view text{
  height:28rpx;
  line-height:28rpx;
  text-align:left;
  padding-left:20rpx;
  color:#ffb400;
}
.detail-banner .detail-banner-up .detail-banner-up-rig .detail-banner-up-rig-four-txt{
  height:24rpx;
  line-height:24rpx;
  text-align:left;
  color:#e8e6e6;
  margin-top:24rpx;
}

.detail-banner .detail-banner-up .detail-banner-up-rig .detail-banner-up-rig-five-txt{
  height:24rpx;
  line-height:24rpx;
  text-align:left;
  color:#e8e6e6;
  margin-top:26rpx;
}
.detail-banner .detail-banner-up .detail-banner-up-rig .detail-banner-up-rig-six-txt{
  height:24rpx;
  line-height:24rpx;
  text-align:left;
  color:#e8e6e6;
  margin-top:24rpx;
}
.detail-banner-up-rig-time{
    color: #fff;
    margin-top: 24rpx;
}
.coutr{
    float:left
}
.screen_year{
    float:left;
    margin-left:20rpx
}

.detail-banner .detail-banner-down{
  width:100%;
  height:60rpx;
  margin-top:20rpx;
}
.detail-banner .detail-banner-down .detail-banner-down-lef-input{
    width:340rpx;
