/* 介绍 */
.introduce-view{
  width:94.66%;
  padding:12rpx 2.67% 0rpx;
  overflow:hidden;
  position:relative;
  color: #818181;
  border-top: 8rpx solid #ebebeb;
  background: #fff;
}
.introduce-view text{
    width:100%;
    line-height:54rpx;
    font-size: 28rpx;
    text-indent: 60rpx;
    word-wrap:break-word;
    overflow:hidden;
    display:block;
    padding-bottom:50rpx;
}
.introduce-view .upDown-view{
  width:94.66%;
  height:40rpx;
  background:#fff;
  position:absolute;
  bottom:0px;
}
.introduce-view .upDown-view image{
  width:18rpx;
  height:10rpx;
  margin-bottom:9rpx;
  margin-left:350rpx;
}
.jian-view{
  width:100%;
  height:8rpx;
  background:#ebebeb;
}
/* 演员 */
.actor-view{
  width:100%;
  overflow:hidden;
  background:#fff;
  
}
.actor-view .actor-view-txt{
  display:block;
  width:97.33%;
  height:70rpx;
  line-height:70rpx;
  border-bottom:2rpx solid #ebebeb;
