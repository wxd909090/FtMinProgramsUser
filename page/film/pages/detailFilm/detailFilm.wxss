}
.actor-view-scroll-view .actor-view-scroll-view-box .bottom-hit-movies-view-img{
  /* background: #3e3e3e; */
  /* background:url("/image/baseMap/baseMap2.jpg"); */
  /* background: url("http://qnbar.oss-cn-hangzhou.aliyuncs.com/ftang/public/baseMap2.jpg"); */
  width: 145rpx; 
  height: 200rpx; 
  display:block;
}
.actor-view-scroll-view .actor-view-scroll-view-box .bottom-hit-movies-view-txt{
    width:145rpx;
    text-align:center;
    color:#4c4c4c;
    display:block;
