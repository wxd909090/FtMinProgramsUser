/* page/merchandiseOrder/merchandiseOrder.wxss */
page{
    background: #fff;
}
.title{
    color: #393939;
    font-size: 30rpx;
    padding: 20rpx 18rpx 20rpx 0;
    border-bottom: 2rpx solid #EBEBEB;
    margin-left: 18rpx;
}
.title image{
    width: 30rpx;
    height: 30rpx;
    margin-right: 17rpx;
}
.title view{
    display: inline-block;
    width: 90%;
    position: relative;
    top: -5rpx;
}
.setfloat{
    float: left;
}
.setbackg{
    border-bottom: 10rpx solid #F3F3F4;
}
.setdiscount{
    height: 100rpx;
    line-height: 100rpx;
    position: relative;
    border-bottom:2rpx solid #EBEBEB;
}
.setdiscount .img{
    position: absolute;
    top: 0;
    left: 0;
    width: 28rpx;
    height: 28rpx;
}
.setdiscount view{
    display: inline-block;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 30rpx;
}
.setdiscount .left{
    color: #343434;
    margin-left: 18rpx;
}
.setdiscount .right{
    color: #969595;
    float: right;
    margin-right: 18rpx;
}
.setdiscount .right image{
    width: 12rpx;
    height: 23rpx;
}
.totalprice{
    height: 104rpx;
    line-height: 104rpx;
    font-size: 25rpx;
}
.totalprice .totalcolor{
    color: #939292;
}
.totalprice view{
    height: 104rpx;
    line-height: 104rpx;
    display: inline-block;
    padding: 0 21rpx;
    float: right;
}
.totalprice .pay{
    color: #393939;
    font-size: 30rpx
}
.totalprice .pay text{
    color: #FFCD2B;
}
.footer{
    position: fixed;
    bottom: 0;
    height: 92rpx;
    line-height: 92rpx;
    position: fixed;
    bottom: 0;
    width: 100%;
    border-top:2rpx solid #EBEBEB;
    background: #fff;
}
.footer .pay{
    font-size: 30rpx;
    color: #FFCD2B;
}
.footer .pay text{
    color: #393939;
}
.footer view{
    display: inline-block;
}
.footer .footerone{
    font-size: 25rpx;
    color: #939292;
}
.footer .pay,.footerone{
    padding: 0 20rpx;
}
.footer .submit{
    /* width: 190rpx;
    height: 92rpx;
    line-height: 92rpx;
    color: white;
    font-weight: 600;
    font-size: 30rpx;
    background: #4BB73B;
    float: right;
    text-align: center; */
    margin-top: 0;
    float: right;
}
.mask{
    /* display: none; */
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.7);
    z-index: 1000;
}
.masktop{
    height: 40%;
    width: 100%;
}
.maskfooter{
    height: 470rpx;
    width: 100%;
}
.maskcontent{
    height: 60%;
    width: 100%;
    background-color: white;
    position: absolute;
    bottom: 0;
    left: 0;
}
.maskcontent image{
    width: 40rpx;
    height: 40rpx;
    vertical-align: middle;
}
.maskcontentone,.maskcontenttwo{
    height: 118rpx;
    color: #1F1F1F;
    font-weight: 400;
    font-size: 30rpx;
    padding: 0 18rpx;
    border-bottom: 2rpx solid #ccc;
}
.maskcontentone{
    line-height: 118rpx;
}
.timList{
    height: 90rpx !important;
    line-height: 90rpx !important;
    border-bottom: 2rpx solid #F4F4F4;
}
.maskcontenttwo{
    line-height: 59rpx;
}
.maskcontentone .left{
  display: inline-block;
  width: 75%;
  float: left;
}
.maskcontentone .right{
  display: inline-block;
  float: right;
}
.maskcontenttwo .left{
  display: inline-block;
  width: 80%;
  float: left;
}
.maskcontenttwo .left .leftone{
  font-size: 22rpx;
  color: #6A6A6A;
}
.maskcontenttwo .right{
  line-height: 118rpx;
  display: inline-block;
  float: right;
}

/* 日期相关 */
.date_box {
    white-space: nowrap;
    width: 100%;
    height: 85rpx;
    background-color: #fff;
    padding: 15rpx 0 0 0;
}

.date_cell {
    display: inline-block;
    width: 29%;
    height: 2.5em;
    text-align: center;
    line-height: 2.5em;
    background-color: #fff;
    color: #000;
    font-size: 26rpx;
    /*border: 1px solid #fff;*/
    border-bottom: 1px solid #ddd;
}
.date_cell_choosed{
    color: #1aad19;
    border-bottom: 1px solid #1aad19;
}
.room_info_wrap{
    position:absolute;
    top:304rpx;
    z-index:999;
    overflow:hidden;
    width:100%;
    padding: 7rpx 0;
    font-size:28rpx;
    color:#fff;
    background:linear-gradient(to bottom, rgba(255,255,255,0) 0%, rgba(0, 0, 0, 0.4) 40%, rgba(0, 0, 0, 0.9) 98%, #FFFFFF 100%);
}
.show_begin_time{
    display:inline-block;
    font-size:32rpx;
}
.show_end_time{
    display:inline-block;
    font-size:26rpx;
    color:#bdbdbd;
    margin-left:4rpx;
    /* position: relative; */
}
.triangleCountNum{
    color: #4BB73B !important;
    background: #fff !important;
}








.content-item{
    width: 100%;
    background-color: #ffffff;
}
.orderInfo{
  background: #fff;
  overflow: hidden;
  padding: 0 3%;
}
.orderFilmInfo{
    clear: both;
    overflow: hidden;
}
.orderFilmInfoLeft{
  float: left;
  font-size: 0;
  margin: 20rpx 0;
}
.filmImg{
  width: 170rpx;
  height: 245rpx;
}
.orderFilmInfoRight{
  float: left;
  margin: 20rpx 0 20rpx 20rpx;
  font-size: 28rpx;
  font-family: "微软雅黑"
}
.filmName{
    height: 60rpx;
    font-family: "微软雅黑";
    line-height: 60rpx;
    font-size: 30rpx;
    font-weight: 600;
    text-overflow:ellipsis;
    white-space:nowrap;
    width: 460rpx;
    overflow: hidden;
}
.orderFilmInfoP{
    height: 56.5rpx;
    line-height: 56.5rpx;
    color: #7e7e7e;
    font-weight: 500;
}
.adress{
    width: 450rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.orderPrice{
    display: inline;
    color: #FFCD2B;
    float: right;
}
.moreTriangleText{
    color: #4BB73B !important;
}
.overHidden{
    overflow: hidden;
}
.triangleInfo{
    overflow:hidden;
    height:120rpx;
    padding:20rpx 20rpx 20rpx 0;
    line-height:60rpx;
    border-bottom: 2rpx solid #EBEBEB;
}
.chooseIcon{
    float:left;
    margin:38rpx 20rpx 0 20rpx
}
.icon{
    width:40rpx;
    height:40rpx
}
.triangleInfoLeft{
    float: left
}
.triangleDes{
    font-size:24rpx;
    line-height: 26rpx;
    color: #cecece;
    width: 320rpx;  
    overflow: hidden;  
}
.triangleInfoRight{
    float:right;
}
.showPrice{
    display:inline;
    color:#cecece;
    font-size:24rpx;
}
.realPrice{
    display:inline;
    color:#FFCD2B;
    font-size: 32rpx;
}
.addTriangleCountBtn{
    width:45rpx;
    height:45rpx;
    background:#4BB73B;
    border-radius:50%;
    line-height:45rpx;
    text-align:center;
    color:#fff;
    float:right;
    margin-top: 8rpx;
}
.showChoosed{
    margin-right : 40rpx;
    font-size: 26rpx !important;
}
.orderInfoComboList{
    height: 60rpx !important;
    text-indent: 20rpx;
}
.timer{
  padding:15rpx 0;
  text-align:center;
  color:#4BB73B;
  background:#fef9b9;
  font-size:26rpx;
}
.price_no{
  height: 30rpx;
  line-height: 30rpx;
  text-decoration:line-through;
  color:#cecece;
}
