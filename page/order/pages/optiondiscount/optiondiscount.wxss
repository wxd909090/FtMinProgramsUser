
@import "../../../../component/alertcoupon/alertcoupon.wxss";
radio {
  transform: scale(0.8);
  float: right;
}
.title {
  height: 100rpx;
  line-height: 100rpx;
  padding-left: 18rpx;
  color: #1f1f1f;
  font-size: 30rpx;
  border-bottom: 2rpx solid #ebebeb;
}
.coupon {
  height: 177rpx;
  line-height: 177rpx;
  padding: 22rpx 18rpx;
  border-bottom: 2rpx solid #ebebeb;
  position: relative;
}
.coupon image{
  width: 200rpx;
  height: 167rpx;
}
.coupon .icon{
  width: 40rpx;
  height: 40rpx; 
}
.coupon .left{
  height: 167rpx;
  width: 650rpx;
  display: inline-block;
  float: left;
  position: relative;
}
.coupon .left .myprice{
  position: absolute;
  left: 20rpx;
  top:0;
  width: 155rpx;
  height: 167rpx;
  text-align: center;
}
.coupon .left .myprice .priceone{
  margin-top: 25rpx;
  width: 76rpx;
  height: 30rpx;
  line-height: 30rpx;
  background-color: #FCE739;
  border-radius: 12rpx;
  font-size: 18rpx;
  color: #66CC57;
  text-align: center;
  margin-left: 36rpx;
}
.coupon .left .myprice .pricetwo{
  width: 155rpx;
  height: 90rpx;
  line-height: 90rpx;
  color: white;
  font-weight: 800;
  font-size: 53rpx;
  margin-top: 15rpx;
}
.coupon .left .myprice .pricetwo text{
  font-size: 21rpx;
}
.coupon .left .backimg{
  position: absolute;
  top: 6rpx;
  left: 0;
  width: 650rpx;
  height: 167rpx;
  z-index: -1;
}
.coupon .middle{
  width: 295rpx;
  display: inline-block;
  position: absolute;
  top: 22rpx;
  left: 266rpx;
  height: 167rpx;
}
.coupon .middle .titlem{
  color: #69CD5A;
  font-size: 25rpx;
  margin-top: 25rpx;
}
.coupon .middle .date{
  color: #C7C7C7;
  font-size:17rpx;
}
.coupon .middle .area{
  color: #909090;
  font-size: 17rpx;
  margin-top: 20rpx;
}
.coupon .middle view{
  height:30rpx;
  line-height: 30rpx;
}
.coupon .right{
  display: inline-block;
  float: left;
  height: 167rpx;
  margin-left: 20rpx;
}
.viewButton{
    position:fixed;
    bottom:0rpx;
    width:100%;
    height:92rpx;
    background:#44b434;
    line-height:92rpx;
    text-align:center;
    color:#fff;
    font-size:34rpx;
}
.couContainer{
  margin-bottom: 120rpx;
}
.inputTop{
    float:left;
    width:80%;
    border:2rpx solid #5fbe51;
    border-radius:8rpx;
    margin-top:26rpx;
    margin-left:2.5%;
    height: 70rpx;
    line-height: 70rpx;
}
.inputPlaceholder{
    line-height: 70rpx;
    text-align: 20rpx;
    font-size: 26rpx;
    text-indent: 10rpx;
}
.inputPlaceholder{
    line-height: 70rpx;
    text-align: 20rpx;
}
.turnText{
    float:right;
    color:#5fbe51;
    margin-top:26rpx;
    margin-right:26rpx;
    height: 70rpx;
    line-height: 70rpx;
}
.tuan{
  margin-top:23rpx;
  width:76rpx;
  line-height:40rpx;
  border-radius:12rpx;
  font-size:30rpx;
  color:#FCE739;
  text-align:center;
  margin-left:36rpx;
  font-weight: 600;
}