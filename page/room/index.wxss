/*日期选择相关*/

.date_box {
  margin-top: 380rpx;
  white-space: nowrap;
  width: 100%;
  height: 85rpx;
  background-color: #fff;
  padding: 15rpx 0 0 0;
}

.date_cell {
  display: inline-block;
  width: 29%;
  height: 2.5em;
  text-align: center;
  line-height: 2.5em;
  background-color: #fff;
  color: #000;
  font-size: 0.95em;
  /*border: 1px solid #fff;*/
  border-bottom: 1px solid #ddd;
}
.date_cell_choosed{
  color: #1aad19;
  border-bottom: 1px solid #1aad19;
}
.room_info_wrap{
  position:absolute;
  top:304rpx;
  z-index:999;
  overflow:hidden;
  width:100%;
  padding: 7rpx 0;
  font-size:28rpx;
  color:#fff;
  background:linear-gradient(to bottom, rgba(255,255,255,0) 0%, rgba(0, 0, 0, 0.4) 40%, rgba(0, 0, 0, 0.9) 98%, #FFFFFF 100%);
}
.show_begin_time{
  display:inline-block;
  font-size:32rpx;
}
.show_end_time{
  display:inline-block;
  font-size:26rpx;
  color:#bdbdbd;
  margin-left:4rpx;
  /* position: relative; */
}
/*预约按钮*/
.order_btn{
  float:right;
  width:5em;
  height:60rpx;
  margin-top:19rpx;
  margin-right:20rpx;
  font-size:28rpx;
  line-height:60rpx;
  color:#fff;
  background:#1aad19;
  border-radius:10rpx;
}
.lock_btn{
  background:#B0B0B0;
}

/*轮播左右箭头*/
.floor{
  position: absolute;
  top: 140rpx;
  width: 100%;
  color: #fff;
}
.left{
  position: absolute;
  top: 150rpx;
  margin-left: 30rpx;
}
.right{
  position: absolute;
  top: 150rpx;
  right: 0;
  margin-right: 30rpx;
}
.swiper-floor-img{
  width: 50rpx;
  height: 50rpx
}
